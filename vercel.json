{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 9 * * *"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}